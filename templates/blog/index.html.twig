{% extends "base.html.twig" %}

{% block title %}Blogs{% endblock %}
{% block body %}
    {% for index, post in posts %}
        <p><a href="{{ path('blog_show', { 'id': index }) }}">ID {{ index }}</a></p>
        <h3> {{ post.title }} </h3>
        <p> {{ post.text }} </p>
    {% endfor %}
{% endblock %}